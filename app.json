{
  "expo": {
    "platforms": ["ios","android","web"],
    "name": "MyJedsCuisine",
    "slug": "MyJedsCuisine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/first-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/first-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/first-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "entryPoint": "./node_modules/expo/AppEntry.js",
    "plugins": [
      "expo-font"
    ]
  }
}
